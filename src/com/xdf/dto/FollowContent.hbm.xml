<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xdf.dto.FollowContent" table="followcon">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="oppId" column="nOppId"></property>
		<property name="time" column="dtTime"></property>
		<property name="content" column="sContent"></property>
		<property name="employee" column="sFollowEmployee"></property>
		<property name="answer" column="sAnswer"></property>
	</class>
</hibernate-mapping>