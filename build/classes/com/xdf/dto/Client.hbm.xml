<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        	<class name="com.xdf.dto.Client" table="client">
        		<id name="id" column="id">
        			<generator class="native"></generator>
        		</id>
        		<property name="oppId" column="nOppId"></property>
        		<property name="followTime" column="dtFollowTime"></property>
        		<property name="nextFollowTime" column="dtNextFollowTime"></property>
        		<property name="state" column="nState"></property>
        	</class>
        </hibernate-mapping>